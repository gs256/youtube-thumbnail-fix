{
    "name": "YouTube Thumbnail Fix",
    "version": "0.1",
    "manifest_version": 3,
    "background": {},
    "permissions": [
        "webRequest",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "rules",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "host_permissions": ["*://*/*"],
    "action": {}
}
